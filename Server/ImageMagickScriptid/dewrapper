Rmax=$1
CenterX=$2
CenterY=$3
start=$(expr $4 - 90)

fin=$5
fout=$6

Rmin=0
end_angle=0 

echo "Input file $fin"
echo "Output file $fout"
echo "R $Rmax X $CenterX Y $CenterY start $start"


if [ "$Rmax" == "0" ]; then
	convert $fin +distort  DePolar 0 $fout
else
	convert $fin +distort  DePolar "$Rmax $Rmin $CenterX $CenterY $start $end_angel" $fout
fi


./label $fout $fout